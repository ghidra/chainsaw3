package chainsaw.ui{	import flash.display.Sprite;	//import flash.display.Stage;		public class window extends Sprite	{			public var palette:Sprite=new Sprite();				//public var _type:String;		//public var _title:String;		//public var _default_w:Number;//the width of the windows		//public var _border:Boolean;//true false on the border		//public var _title_vis:Boolean;//does it display the title		//public var _stack:Boolean;//is it in a stack		//public var stackStart:Number=2;//where to start the stack				public function window():void{//ty:type, t:title, tv:title visible 0 or 1, B:border 0 or 1.			addChild(palette);		}		public function draw_window(x:Number=0,y:Number=0,w:Number=40,h:Number=12):void{			palette.graphics.clear();		 	palette.graphics.beginFill(0xD4D4D4,0.4); // grey color			palette.graphics.drawRoundRect(x, y, w, h, 10, 10); // x, y, width, height, ellipseW, ellipseH		  	palette.graphics.endFill();		}				//------------making windows	}	}