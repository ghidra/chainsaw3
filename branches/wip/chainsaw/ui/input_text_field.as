package chainsaw.ui{		import chainsaw.ui.bg;	import chainsaw.ui.button;	import chainsaw.ui.ui_text;	public class input_text_field extends bg	{		public var button_obj:button;		public var text_obj:ui_text;				public function input_text_field(i:Number,s:String,v:String="",x:Number=0,y:Number=0,w:Number=100,h:Number=12):void{						button_obj=new button(i,s,new Array(),null,x,y,40);			text_obj= new ui_text(v,42,y,w-42,h+2,"INPUT");						draw_bg(42,y,w-42,h);//draw the button			addChild( button_obj );			addChild(text_obj);		}			}}