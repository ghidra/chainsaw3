package chainsaw.primitive{	import flash.utils.*;		import chainsaw.math.matrix4;	import chainsaw.geometry.geometry;	public class primitive extends geometry{		public var _id			: 	String;		public var _on_complete	:	Function;				public var _interval	:	uint;				public function primitive():void//scn:scene//on_complete_function:Function					{		}		public function setTimed():void		{			_interval 			= 	setTimeout(timedFunction, .0001);		}		public function timedFunction():void		{			clearInterval(_interval)			_on_complete();		}	}	}