package{	import flash.display.Sprite;	import flash.display.Shape;	import flash.display.Stage;		import flash.events.Event;		import history.ui.input;		import chainsaw.object.scene;	import chainsaw.object.camera;	import chainsaw.object.renderer;	import chainsaw.object.obj;	import chainsaw.object.obj_disc;	import chainsaw.object.obj_instance;	import chainsaw.ui.userinput;	public class history_main extends Sprite	{		public var input1	: input;				public var scene1	:	scene;									//create a scene to hold everything		public var camera1	:	camera;									//camera		public var renderer1:	renderer;								//renderer		public var user1	:	userinput;				//private var the_mang:Number=0;		//-----------------		public function history_main():void{			input1	=	new input();			addChild(input1);						scene1 		= 	new scene();					//set up a scene			camera1		=	new camera(380,550,400,2,100);	//new camera(Stage.width,Stage.height,380,1,100);			renderer1	= 	new renderer(scene1,camera1);	//ret up the renderer			user1 		=	new userinput(renderer1,stage);			addChild(renderer1);						//scene1._objects.push(camera1);//add the camera to the scene objects			scene1._init = [	["crazy_head",obj],["3,20",obj_disc]	];											//array of external objects to load			scene1._inst = [	[0,obj_instance,[0,0,-10]] , [1,obj_instance,[0,10,-10]]	];						//scene1._init = [	["the_mang",obj],/*["planet_01",obj],*/["crazy_head",obj],["4,6",obj_disc]	];											//array of external objects to load			//scene1._inst = [	[0,obj_instance,[0,0,-10]],/*[0,obj_instance,[0,0,0]], */ [1,obj_instance,[10,0,-20]],[1,obj_instance,[6,4,-15]]/*,[1,obj_instance,[0,-5,-30]],[1,obj_instance,[-5,5,-10]],[2,obj_instance,[0,0,0]]*/		];			scene1.init(run);												//start loding one at a time						function run():void{				user1._refresh();								scene1._scene[0].rotateY(.01);				scene1._scene[0].rotateZ(.01);								scene1._scene[1].rotateY(.02);				scene1._scene[1].rotateZ(.01);								renderer1.renderscene();			}					}		//-----------------		//-----------------	}}