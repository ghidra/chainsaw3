package{	import flash.display.Sprite;	import flash.display.Stage;		import flash.events.Event;		import chainsaw.scene.scene;	import chainsaw.primitive.*;	import chainsaw.instance.instance;	import chainsaw.ui.userinput;	public class procedural_tree extends Sprite	{		public var scene1	:	scene;									//create a scene to hold everything		public var user1	:	userinput;		//-----------------		public function procedural_tree():void{						scene1 		= 	new scene();					//set up a scene			user1 		=	new userinput(scene1._renderer,stage);			addChild(scene1);						//scene1._init = [	["3,20",disc],["crazy_head",obj]	];											//array of external objects to load			//scene1._inst = [	[0,instance,[0,10,-10]],[1,instance,[0,0,-10]] /*,[1,obj_instance,[0,-5,-30]],[1,obj_instance,[-5,5,-10]],[2,obj_instance,[0,0,0]]*/		];			scene1._init = [	["crazy_head",obj],["5,20",disc],["the_mang",obj],["",ptree]	];											//array of external objects to load			scene1._inst = [	[0,instance,[0,0,-10]],[1,instance,[5,0,-10]],[2,instance,[5,0,-14]],[3,instance,[-10,0,-10]] ];			scene1.init(run);												//start loding one at a time, and send it the function to run every frame after done loading						function run():void{				user1._refresh();								scene1._scene[0].rotateY(.01);				scene1._scene[0].rotateZ(.01);								scene1._scene[1].rotateY(.02);				scene1._scene[1].rotateZ(.01);								scene1.render();			}		}		//-----------------		//-----------------	}}